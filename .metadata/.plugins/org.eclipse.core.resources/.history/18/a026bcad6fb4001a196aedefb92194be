package com.applications.assertions;

import org.testng.annotations.Test;

public class TimeOutInMethods {
	
	@Test(timeOut=1000)
	public void test1()
	{
		System.out.println("Test1:");
		for(;;)
		{
			System.out.println("welcocme to fast and easy lerning");
		}
	}
	
	@Test
	public void test2()
	{
		System.out.println("Test2:");
	}

}
