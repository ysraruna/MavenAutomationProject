public class DataProviderTest {
	
	@Test
	public void sumOfTwoNumbers(int x,int y)
	{
		int sum=x+y;
		System.out.println("sum is:"+sum);
	}
	
	@DataProvider(name="numberInputs")
	public Object[][] getDataFromProvider()
	{
		return new Object[][];
		
	}

}
